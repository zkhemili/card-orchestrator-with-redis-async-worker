
#Server
PORT=8080 
LOG_LEVEL=info 
LOG_SAMPLE_RATE=1

#Redis / Queue
REDIS_URL=redis://127.0.0.1:6379 
QUEUE_NAME=card-orchestrator
JOB_PREFIX=job: 
JOB_TTL_SECONDS=86400

#Worker tuning
WORKER_CONCURRENCY=5 
WORKER_LIMITER_MAX=150
WORKER_LIMITER_DURATION_MS=60000

#Config
CONFIG_PATH=./config.json

#cc-s3
S3_API_BASE=https://api.cc-s3.net 
S3_API_KEY=YOUR_CC_S3_KEY
CSV_PREFIX=cardTemplates/ramadan 
CSV_USE_PREFIX=false

#Adobe
ADOBE_CLIENT_ID=YOUR_ADOBE_CLIENT_ID
ADOBE_CLIENT_SECRET=YOUR_ADOBE_CLIENT_SECRET POLL_INTERVAL_MS=2000
POLL_TIMEOUT_MS=120000

#Fonts
FONT_DEST_DIR=fonts
